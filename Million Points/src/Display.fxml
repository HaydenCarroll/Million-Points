<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="560.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
   <bottom>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="enter" layoutX="642.0" layoutY="16.0" mnemonicParsing="false" onAction="#populate" text="Enter" />
            <Button layoutX="590.0" layoutY="14.0" mnemonicParsing="false" onAction="#play" text="Play" />
         </children>
      </AnchorPane>
   </bottom>
   <center>
      <Pane fx:id="pane" onContextMenuRequested="#drag" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
