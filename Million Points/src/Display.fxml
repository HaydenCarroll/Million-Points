<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="560.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main">
   <bottom>
      <AnchorPane BorderPane.alignment="CENTER">
         <children>
            <Slider fx:id="dotCount" layoutX="56.0" layoutY="15.0" majorTickUnit="50.0" max="500.0" min="1.0" minorTickCount="50" showTickLabels="true" showTickMarks="true" snapToTicks="true" />
            <Label layoutX="83.0" layoutY="3.0" text="Number of Dots" />
            <Button fx:id="enter" layoutX="642.0" layoutY="16.0" mnemonicParsing="false" onAction="#populate" text="Enter" />
            <Button layoutX="590.0" layoutY="14.0" mnemonicParsing="false" onAction="#play" text="Play" />
            <Button layoutX="531.0" layoutY="14.0" mnemonicParsing="false" onAction="#pause" text="Pause" />
         </children>
      </AnchorPane>
   </bottom>
   <center>
      <Pane fx:id="pane" prefHeight="1080.0" prefWidth="1920.0" style="-fx-background-color: #000000;" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
